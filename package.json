{
  "name": "weav-discord",
  "version": "2.0.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Chris Weaver",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisWeaver1/weav-discord"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "build:ts": "tsc -p examples/typescript",
    "start": "tsc && tsc -p examples/typescript && node examples/typescript/src/bot.js",
    "config": "echo '{\"token\":\"\",\"prefixes\":[]}' > examples/config.json",
    "start:js": "node examples/javascript/bot.js",
    "docker:build": "docker build . -t weav-discord",
    "docker:run": "docker run -d --name weav-discord weav-discord",
    "docker:run-attached": "docker run --name weav-discord weav-discord",
    "docker:stop": "docker container stop weav-discord",
    "docker:remove": "docker container rm weav-discord",
    "docker": "npm run docker:build && npm run docker:stop; npm run docker:remove; npm run docker:run",
    "docker:attached": "npm run docker:build && npm run docker:stop; npm run docker:remove; npm run docker:run-attached"
  },
  "dependencies": {
    "@types/node": "^10.12.9",
    "discord.js": "^11.4.2",
    "typescript": "^2.9.2"
  },
  "keywords": [
    "discord",
    "bot",
    "framework",
    "router",
    "paging",
    "arguments",
    "controller"
  ]
}
